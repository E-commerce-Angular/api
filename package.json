  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^6.2.1",
    "express": "^4.17.1",
    "mongoose": "^5.13.7",
    "nodemon": "^2.0.12",
    "require-dir": "^1.2.0"
  },
  "devDependencies": {
    "@types/node": "^16.6.2",
    "typescript": "4.3.4"
  }
}
